{
  "name": "triviab",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:all": "pnpm --filter '**' dev",
    "dev:ws": "pnpm --filter ws-server dev",
    "dev:app": "dotenv -- pnpm --filter triviab-app dev",
    "build:all": "pnpm --filter '**' build",
    "build:ws": "pnpm --filter ws-server build",
    "build:app": "pnpm --filter triviab-app build",
    "build:types": "pnpm --filter types build",
    "start:all": "pnpm --filter '**' start",
    "start:ws": "pnpm --filter ws-server start",
    "start:app": "pnpm --filter triviab-app start",
    "lint": "pnpm --filter '**' lint"
  },
  "dependencies": {
    "@libsql/client": "0.15.12",
    "dotenv-cli": "10.0.0",
    "ioredis": "5.7.0",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.34.0",
    "@types/node": "22.17.2",
    "eslint": "9",
    "typescript": "5"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
